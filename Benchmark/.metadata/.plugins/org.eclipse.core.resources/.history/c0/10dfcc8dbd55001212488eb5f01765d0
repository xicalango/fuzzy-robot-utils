import java.util.ArrayList;


public class test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		ArrayList<URL_Freq> l = new ArrayList<URL_Freq>();
		
		l.add(new URL_Freq(new UrlInfo("test1"), 20));
		l.add(new URL_Freq(new UrlInfo("test2"), 20));
		l.add(new URL_Freq(new UrlInfo("test3"), 20));
		l.add(new URL_Freq(new UrlInfo("test4"), 30));
		l.add(new URL_Freq(new UrlInfo("test5"), 10));
		
		CardDeck d = new CardDeck(l);
		d.printUnusedCards();
		
		int array [] = {0,0,0,0,0};
		int i = 0;
		while(i<1000000000){
		if(d.getNextCard().toString().equals("test1")){
			array[0]++;
		}
		if(d.getNextCard().toString().equals("test2")){
			array[1]++;
		}
		if(d.getNextCard().toString().equals("test3")){
			array[2]++;
		}
		if(d.getNextCard().toString().equals("test4")){
			array[3]++;
		}
		if(d.getNextCard().toString().equals("test5")){
			array[4]++;
		}
		i++;
		}
		System.out.println("Erg: \n");
		for(i=0;i<array.length;i++){
			System.out.println(array[i]);
		}
	}

}
